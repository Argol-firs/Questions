#include <iostream>
#include "getName.h"
#include <string>
#include <locale.h>
using namespace std;

int a, b, f, x;

int main()
{
	setlocale(LC_ALL, "Russian");
	cout << "Введите числа а, b, f: " << endl;
	cin >> a >> b >> f;
	cout << "Число a = " + a + "Число b = " + b + "Число f = " + f << endl;
	x = (a + b - f / a) + f * a * a - (a + b);
	cout << "Произведем расчет по формуле (а + b — f / а) + f * a * a — (a + b)" << endl;
	cout << "Ответ: " << x << endl;
	return 0;
}
